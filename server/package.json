{
  "name": "@tilemud/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --esm src/index.ts",
    "build": "tsc -p tsconfig.json",
  "test": "vitest --run --config ./vitest.config.ts",
  "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\"",
  "migrate": "node dist/scripts/run-migrations.js",
  "seed:ruleset": "node dist/scripts/seed-ruleset.js",
  "purge:messages": "node dist/scripts/purge-private-messages.js"
  },
  "dependencies": {
  "@colyseus/schema": "3.0.61",
    "colyseus": "0.16.4",
    "express": "5.0.0",
    "pino": "8.19.0",
    "pg": "8.11.5",
    "redis": "5.0.1",
    "uuid": "9.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/pg": "8.11.10",
    "@types/supertest": "2.0.16",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "openapi-typescript": "6.7.1",
    "prettier": "3.2.5",
    "supertest": "7.1.3",
    "ts-node-dev": "2.0.0",
    "tsx": "4.7.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  }
}
